.clearf:after {
    content: ".";
    display: block;
    clear: both;
    visibility: hidden;
    line-height: 0;
    height: 0;
}
 
.clearf {
    display: inline-block;
}

#jw_<%= @gist.id %> {
  margin: 1%;
  border: 1px #ccc solid;
}

#jw_<%= @gist.id %> .jw_container{
  font: 100%;
  overflow-y: hidden;
  position: relative;
}


#jw_<%= @gist.id %> .jw_line_numbers{
  font-family: 'Monaco', 'Menlo', 'Ubuntu Mono', 'Consolas', 'source-code-pro', monospace;
  font-size: 11px;
  float: left;
  width: 43px;
  border-right: 2px #999 solid;
  padding: 2.2px 10px;
  text-align: right;
  background-color: #eee;
  color: #777;
}

#jw_<%= @gist.id %> .jw_line_numbers .jw_number {
  display: block;
}


#jw_<%= @gist.id %> .jw_content {
  white-space: pre;
  font-family: 'Monaco', 'Menlo', 'Ubuntu Mono', 'Consolas', 'source-code-pro', monospace;
  font-size: 11px;
  width: 200px;
}

#jw_<%= @gist.id %> .jw_image {
  /*width: 100%;*/
  /*height: 100%;*/
  z-index: -2;
  position: absolute;
  top: 0;
  left: 0;
  /*border: red 1px solid;*/
  overflow: hidden;
}

#jw_<%= @gist.id %> .jw_footer{
  clear: both;
  padding: 10px;
  background: #ccc;
  width: 100%;
}


